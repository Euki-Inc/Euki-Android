<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    android:padding="@dimen/dimen_x3_5">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Contraception"
        style="@style/TextStyle9"/>
    
    <LinearLayout
        android:id="@+id/ll_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen_x3_5"
        android:layout_marginBottom="@dimen/dimen_x1_5"
        android:orientation="vertical"/>

    <Button
        android:id="@+id/b_set_reminders"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/dimen_x2_5"
        android:paddingBottom="@dimen/dimen_x2_5"
        android:gravity="center"
        android:text="@string/set_reminders"
        android:background="@null"
        style="@style/LinkOrButtonInactiveOrSecondary"/>

</LinearLayout>
